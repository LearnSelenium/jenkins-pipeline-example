pipleline {
    agent any
    stages{
        stage('Compile Stage'){

            steps{
                witMaven(maven: 'Maven'){
                    sh 'mvn clean compile'
                }
            }
        }

        stage('Testing State'{
            steps{
                withMaven(maven:'Maven'){
                    sh 'mvn test'
                }
            }
        }

        stage{
            steps{
                withMaven(maven:'Maven'){
                    sh 'mvn deploy'
                }
            }
        }
    }
}